    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "umi-plugin-circular-check": "^1.0.0",
    "update-browserslist-db": "^1.1.1",
    "wait-port": "^1.1.0",
    "web-vitals": "^4.2.3",
    "yorkie": "^2.0.0"
  },
  "packageManager": "yarn@4.5.0",
  "engines": {
