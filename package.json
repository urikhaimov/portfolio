{
  "homepage": "https://teamco.github.io/portfolio/",
  "private": true,
  "name": "portfolio",
  "version": "0.0.1",
  "scripts": {
    "start": "max dev",
    "start:devtools": "pm2 start ecosystem.config.js",
    "start:debug": "cross-env DEBUG=1 max dev",
    "analize": "cross-env ANALYZE=1 max build",
    "wait:port": "wait-port 8003",
    "umi:envs": "cross-env NODE_ENV=development node -r esm ./config/envs.js",
    "umi:routes": "cross-env NODE_ENV=development node -r esm ./config/routes.js",
    "umi:proxy": "cross-env NODE_ENV=development node -r esm ./config/proxy.js",
    "umi:alias": "cross-env NODE_ENV=development node -r esm ./config/alias.js",
    "umi:models": "max dva list model",
    "umi:deadcode": "max deadcode",
    "umi:plugins": "max plugin list",
    "umi:preview": "max preview --port 9527",
    "umi:mfsu:ls": "max mfsu ls",
    "umi:mfsu:build": "max mfsu build",
    "umi:mfsu:remove": "max mfsu remove",
    "umi:mfsu:remove:all": "max mfsu remove --all",
    "license:development": "license-checker --json > license.development.json",
    "license:production": "license-checker --production --json > license.production.json",
    "version:update": "yarn set version stable",
    "libs:rebuild": "yarn rebuild",
    "libs:update": "yarn upgrade-interactive",
    "umi:lint": "max lint",
    "umi:lint:fix": "max lint --fix",
    "cspell": "cspell ./src/**/*.js",
    "knip": "knip --production --config knip.json",
    "update:browsers": "npx update-browserslist-db@latest",
    "build:verification": "serve ./dist",
    "build": "max build",
    "build:docs": "node_modules/.bin/jsdoc -c jsdoc-conf.json",
    "mdocs:services": "jsdoc2md ./src/services/*.js > ./src/services/index.md",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d dist",
    "postdeploy": "now",
    "precommit": "lint-staged",
    "js2flowchart": "js2flowchart ./src/layouts/landing/landing.layout.jsx",
    "madge": "madge --image graph.svg --extensions js ./src",
    "unit:test": "jest --passWithNoTests",
    "unit:test:coverage": "jest --passWithNoTests --coverage",
    "single:suite": "jest --passWithNoTests ./src/components/Footer/__tests__/footer.test.js",
    "single:test": "jest --passWithNoTests ./src/components/Common/Tags/__tests__/common.tags.test.js -t 'Hooked tags'"
  },
  "lint-staged": {
    "*.{js,jsx,css,less}": [
      "max lint --fix",
      "git add",
      "git commit -am 'lint-staged'"
    ]
  },
  "bugs": {
    "url": "https://github.com/teamco/portfolio/issues",
    "email": "teamco@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:teamco/portfolio.git"
  },
